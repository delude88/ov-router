{
  "name": "@digitalstage/ov-router",
  "version": "0.0.1",
  "description": "Routing service to provide ov multicast processes",
  "main": "dist/index.js",
  "gypfile": true,
  "scripts": {
    "dev": "DEBUG=* nodemon --watch './src/**/*.ts' --exec 'ts-node' src/index.ts | pino-colada",
    "lint": "npx eslint ./src --ext .js,.ts",
    "prepare": "make -C libov",
    "build": "source version.sh && node-gyp rebuild --verbose && tsc",
    "start": "tsc && node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-stage/router-distributor.git"
  },
  "keywords": [
    "video",
    "realtime",
    "mediasoup",
    "node"
  ],
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digital-stage/router-distributor/issues"
  },
  "homepage": "https://github.com/digital-stage/router-distributor#readme",
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "pino-colada": "^2.1.0",
    "pm2": "^4.5.0",
    "process": "^0.11.10",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "mongodb": "^3.6.3",
    "node": "^15.2.0",
    "node-addon-api": "^3.1.0",
    "node-fetch": "^2.6.1",
    "node-gyp": "^7.1.2",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "teckos": "0.2.3"
  }
}
